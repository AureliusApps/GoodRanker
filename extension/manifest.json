{
  "name": "GoodRanker",
  "description": "Helps rank goodreads books!",
  "version": "1.0",
  "manifest_version": 3,
  "background" : {
    "service_worker" : "background.js"
  },
  "permissions" : ["storage", "activeTab", "scripting"],
  "action" : {
    "default_popup" : "popup.html",
    "default_icon" : {
      "16" : "/images/zen.png"
    }
  },
  "icons" : {
    "16" : "/images/zen.png"
  }
}
